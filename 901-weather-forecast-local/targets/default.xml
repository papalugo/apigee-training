<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="default">
  <PreFlow name="PreFlow">
    <Request/>
    <Response/>
  </PreFlow>
  <Flows/>
  <PostFlow name="PostFlow">
    <Request/>
    <Response>
      <Step>
        <Name>EV-1</Name>
      </Step>
      <Step>
        <Name>AM-1</Name>
      </Step>
    </Response>
  </PostFlow>
  <HTTPTargetConnection>
    <URL>https://api.openweathermap.org/data/2.5/weather?q={apigee.local}&amp;appid={apigee.apikey}</URL>
  </HTTPTargetConnection>
</TargetEndpoint>
