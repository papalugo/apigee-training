<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="default">
  <Description/>
  <FaultRules/>
  <PreFlow name="PreFlow">
    <Request/>
    <Response/>
  </PreFlow>
  <PostFlow name="PostFlow">
    <Request/>
    <Response>
      <Step>
        <Name>AM-1</Name>
      </Step>
      <Step>
        <Name>JWT-1</Name>
      </Step>
      <Step>
        <Name>AM-2</Name>
      </Step>
    </Response>
  </PostFlow>
  <Flows/>
  <HTTPProxyConnection>
    <BasePath>/jwt-generate</BasePath>
    <Properties/>
    <VirtualHost>default</VirtualHost>
  </HTTPProxyConnection>
  <RouteRule name="default"/>
</ProxyEndpoint>
